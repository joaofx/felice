@model IEnumerable<Project>

<h2>Projects</h2>

<p>
    @Html.ActionLink("New Project", "New", "Project")
</p>

<table>
    <thead> 
        <tr>
            <th>Name</th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var project in Model)
        {
            <tr>
                <td>@project.Name</td>
            </tr>
        }
    </tbody>
</table>